<?xml version="1.0" encoding="UTF-8"?>
<settings
        xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
        xmlns="http://maven.apache.org/SETTINGS/1.1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <profiles>
        <profile>
            <id>gcp</id>
            <repositories>
                <repository>
                    <id>gcp-releases</id>
                    <url>artifactregistry://us-maven.pkg.dev/foreman-production/releases</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>gcp-snapshots</id>
                    <url>artifactregistry://us-maven.pkg.dev/foreman-production/snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>gcp-releases</id>
                    <url>artifactregistry://us-maven.pkg.dev/foreman-production/releases</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <snapshots/>
                    <id>gcp-snapshots</id>
                    <url>artifactregistry://us-maven.pkg.dev/foreman-production/snapshots</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>gcp</activeProfile>
    </activeProfiles>
</settings>